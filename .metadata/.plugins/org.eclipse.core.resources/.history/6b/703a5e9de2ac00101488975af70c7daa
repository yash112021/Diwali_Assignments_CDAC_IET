package com.demo.dao;

public class DaoInterfaceImpl  implements DaoInterface {

	
	// writing the logic part for the checkprime 
	@Override
	public boolean checkPrime(int n) {
		if(n<=1) {
			return false;
		}
		if(n==2 ) {
			return true;
		}
		//for loop 
		for(int i=2; i<n;i++) {
			if(n%i==0) {
				return false;
			}
		}
		return true ;
	}

	
	// factorail of A Number
	@Override
	public int fact(int z) {
		int sum=1;
		for(int i=z;i>=1;i--) {
			sum=sum *i;
		}
		return sum;
	}

	// fibonanci series
	@Override
	public int[] resfib(int n) {
		// creating and temporary array of the same size
		int temp[]=new int [n];
		int a=0;
		int b=1;
		temp[0]=0;
		temp[1]=1;
		for(int i=2;i<n;i++) {	
			int sum=a+b;
			temp[i]=sum;
			a=b;
			b=sum;
		}
		
		return temp;
	}

	//even odd Number
	@Override
	public boolean evenOdd(int n) {
		if(n%2==0) {
			return true;
		}
		return false;
	}


	@Override
	public int[] primeNumberArr(int n) {
		// TODO Auto-generated method stub
		return null;
	}
	
	
		

}
